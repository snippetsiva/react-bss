{
  "name": "bss-cbs",
  "version": "5.1.1",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server --config webpack.dev.js",
    "build": "npm run lingui-compile && webpack --progress --config webpack.prod.js",
    "build-package": "npm run lingui-compile && webpack --progress --config webpack.package.js",
    "build:dev": "webpack --progress --config webpack.dev.js",
    "lint": "eslint . --ext .js,.jsx --fix",
    "lingui-add-locale": "lingui add-locale",
    "lingui-extract": "lingui extract --clean",
    "lingui-compile": "lingui compile",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "test-all": "cypress run",
    "test:local": "cypress open --env testMode=local",
    "test:dev": "cypress open --env testMode=dev --config baseUrl=https://dmld-dev.tecnotree.com",
    "test:int": "cypress open --env testMode=int --config baseUrl=http://dmld-mmp.cluster1.devtestlab2.tecnotree.com",
    "test-update": "jest --updateSnapshot --colors",
    "pretest-report": "rm -rf cypress/reports/* || true",
    "test-report": "cypress run --browser=chrome --spec $PATH",
    "pretest:generate-multi-report": "mochawesome-merge --reportDir cypress/reports/*.json > cypress/reports/output.json",
    "test:generate-multi-report": "marge cypress/reports/output.json --reportDir ./ --inline"
  },
